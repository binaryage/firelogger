<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>firelogger@hildebrand.cz</em:id>
    <em:version>1.1</em:version>
    <em:type>2</em:type>
    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.5</em:minVersion>
        <em:maxVersion>6.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <!-- Firebug extension -->
    <em:requires>
      <Description>
        <em:id>firebug@software.joehewitt.com</em:id>
        <em:minVersion>1.4</em:minVersion>
        <em:maxVersion>1.*</em:maxVersion>
      </Description>
    </em:requires>
    <em:name>FireLogger</em:name>
    <em:description>Server-side logging support for web developers</em:description>
    <em:creator>Antonin Hildebrand</em:creator>
    <em:homepageURL>http://firelogger.binaryage.com</em:homepageURL>
    <em:iconURL>chrome://firelogger/skin/icon.png</em:iconURL>
  </Description>
</RDF>