#teaser.overlay
    background-image url('/shared/img2/overlay-664x400.png')
    width 664px
    height 400px

    .overlay-content
        text-align center
        z-index 20000
        absolute(left 11px top 9px)
        
    .overlay-content object
        display block
        border 1px solid #444
        border-radius 4px
        
    .close
        background-image url('/shared/img2/overlay-close.png')
        absolute(left 16px top 16px)
        z-index 30000
        cursor pointer
        height 13px
        width 13px